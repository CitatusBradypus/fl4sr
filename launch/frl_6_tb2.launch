<launch>
  <arg name="gui"        default="true"/>
  <arg name="world_file" default="$(env TURTLEBOT_GAZEBO_WORLD_FILE)"/>

  <arg name="base"       value="$(optenv TURTLEBOT_BASE kobuki)"/> <!-- create, roomba -->
  <arg name="battery"    value="$(optenv TURTLEBOT_BATTERY /proc/acpi/battery/BAT0)"/>  <!-- /proc/acpi/battery/BAT0 -->
  <arg name="stacks"     value="$(optenv TURTLEBOT_STACKS hexagons)"/>  <!-- circles, hexagons -->
  <arg name="3d_sensor"  value="$(optenv TURTLEBOT_3D_SENSOR asus_xtion_pro)"/>  <!-- kinect, asus_xtion_pro -->
  <arg name="lidar" value = "$(optenv TURTLEBOT_LIDAR rplidar)" /> 


  <arg name="first_tb2"  default="tb2_0"/>
  <arg name="second_tb2" default="tb2_1"/>
  <arg name="third_tb2" default="tb2_2"/>
  <arg name="fourth_tb2" default="tb2_3"/>
  <arg name="fifth_tb2" default="tb2_4" />
  <arg name="sixth_tb2" default="tb2_5" />

  <arg name="x_pos1" default="10.0"/>
  <arg name="y_pos1" default="9.0"/>
  <arg name="z_pos1" default="0.0"/>
  <arg name="yaw_1"  default="3.14"/>

  <arg name="x_pos2" default="1.7"/>
  <arg name="y_pos2" default="9.0"/>
  <arg name="z_pos2" default="0.0"/>
  <arg name="yaw_2"  default="0.0"/>

  <arg name="x_pos3" default="-6.0"/>
  <arg name="y_pos3" default="9.0"/>
  <arg name="z_pos3" default="0.0"/>
  <arg name="yaw_3"  default="3.14"/>

  <arg name="x_pos4" default="12.0"/>
  <arg name="y_pos4" default="-9.0"/>
  <arg name="z_pos4" default="0.0"/>
  <arg name="yaw_4"  default="0.0"/>

  <arg name="x_pos5" default="0.0"/>
  <arg name="y_pos5" default="-9.0"/>
  <arg name="z_pos5" default="0.0"/>
  <arg name="yaw_5"  default="3.14"/>

  <arg name="x_pos6" default="-12.0"/>
  <arg name="y_pos6" default="-9.0"/>
  <arg name="z_pos6" default="0.0"/>
  <arg name="yaw_6"  default="0.0"/>


  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find fl4sr)/worlds/frl_ma.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <group ns = "$(arg first_tb2)">


    <include file="$(find turtlebot_gazebo)/launch/includes/$(arg base)_fix.launch.xml">
      <arg name="base" value="$(arg base)"/>
      <arg name="stacks" value="$(arg stacks)"/>
      <arg name="3d_sensor" value="$(arg 3d_sensor)"/>
      <arg name="lidar" value="$(arg lidar)" />
      <arg name="model_name" value="$(arg first_tb2)"/>
      <arg name="x_pos" value="$(arg x_pos1)" />
      <arg name="y_pos" value="$(arg y_pos1)" />
      <arg name="z_pos" value="$(arg z_pos1)" />
      <arg name="yaw" value="$(arg yaw_1)" />
    </include>
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen">
      <param name="publish_frequency" type="double" value="50.0" />
      <param name="tf_prefix" value="$(arg first_tb2)" />
    </node>
    
  </group>

  <group ns = "$(arg second_tb2)">
    <include file="$(find turtlebot_gazebo)/launch/includes/$(arg base)_fix.launch.xml">
      <arg name="base" value="$(arg base)"/>
      <arg name="stacks" value="$(arg stacks)"/>
      <arg name="3d_sensor" value="$(arg 3d_sensor)"/>
      <arg name="lidar" value="$(arg lidar)" />
      <arg name="model_name" value="$(arg second_tb2)"/>
      <arg name="x_pos" value="$(arg x_pos2)" />
      <arg name="y_pos" value="$(arg y_pos2)" />
      <arg name="z_pos" value="$(arg z_pos2)" />
      <arg name="yaw" value="$(arg yaw_2)" />
    </include>
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen">
      <param name="publish_frequency" type="double" value="50.0" />
      <param name="tf_prefix" value="$(arg second_tb2)" />
    </node>
  </group>

  <group ns = "$(arg third_tb2)">
    <include file="$(find turtlebot_gazebo)/launch/includes/$(arg base)_fix.launch.xml">
      <arg name="base" value="$(arg base)"/>
      <arg name="stacks" value="$(arg stacks)"/>
      <arg name="3d_sensor" value="$(arg 3d_sensor)"/>
      <arg name="lidar" value="$(arg lidar)" />
      <arg name="model_name" value="$(arg third_tb2)"/>
      <arg name="x_pos" value="$(arg x_pos3)" />
      <arg name="y_pos" value="$(arg y_pos3)" />
      <arg name="z_pos" value="$(arg z_pos3)" />
      <arg name="yaw" value="$(arg yaw_3)" />
    </include>
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen">
      <param name="publish_frequency" type="double" value="50.0" />
      <param name="tf_prefix" value="$(arg third_tb2)" />
    </node>
  </group>

  <group ns = "$(arg fourth_tb2)">
    <include file="$(find turtlebot_gazebo)/launch/includes/$(arg base)_fix.launch.xml">
      <arg name="base" value="$(arg base)"/>
      <arg name="stacks" value="$(arg stacks)"/>
      <arg name="3d_sensor" value="$(arg 3d_sensor)"/>
      <arg name="lidar" value="$(arg lidar)" />
      <arg name="model_name" value="$(arg fourth_tb2)"/>
      <arg name="x_pos" value="$(arg x_pos4)" />
      <arg name="y_pos" value="$(arg y_pos4)" />
      <arg name="z_pos" value="$(arg z_pos4)" />
      <arg name="yaw" value="$(arg yaw_4)" />
    </include>
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen">
      <param name="publish_frequency" type="double" value="50.0" />
      <param name="tf_prefix" value="$(arg fourth_tb2)" />
    </node>
  </group>


<group ns = "$(arg fifth_tb2)">
    <include file="$(find turtlebot_gazebo)/launch/includes/$(arg base)_fix.launch.xml">
      <arg name="base" value="$(arg base)"/>
      <arg name="stacks" value="$(arg stacks)"/>
      <arg name="3d_sensor" value="$(arg 3d_sensor)"/>
      <arg name="lidar" value="$(arg lidar)" />
      <arg name="model_name" value="$(arg fifth_tb2)"/>
      <arg name="x_pos" value="$(arg x_pos5)" />
      <arg name="y_pos" value="$(arg y_pos5)" />
      <arg name="z_pos" value="$(arg z_pos5)" />
      <arg name="yaw" value="$(arg yaw_5)" />
    </include>
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen">
      <param name="publish_frequency" type="double" value="50.0" />
      <param name="tf_prefix" value="$(arg fifth_tb2)" />
    </node>
  </group>


<group ns = "$(arg sixth_tb2)">
   <include file="$(find turtlebot_gazebo)/launch/includes/$(arg base)_fix.launch.xml">
      <arg name="base" value="$(arg base)"/>
      <arg name="stacks" value="$(arg stacks)"/>
      <arg name="3d_sensor" value="$(arg 3d_sensor)"/>
      <arg name="lidar" value="$(arg lidar)" />
      <arg name="model_name" value="$(arg sixth_tb2)"/>
      <arg name="x_pos" value="$(arg x_pos6)" />
      <arg name="y_pos" value="$(arg y_pos6)" />
      <arg name="z_pos" value="$(arg z_pos6)" />
      <arg name="yaw" value="$(arg yaw_6)" />
    </include>
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen">
      <param name="publish_frequency" type="double" value="50.0" />
      <param name="tf_prefix" value="$(arg sixth_tb2)" />
    </node>
  </group>
</launch>


